<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.glxy.sass.mapper.UsersSortMapper">

<!--  统计男女报名数量-->
    <select id="selectSum" resultType="org.glxy.sass.entity.VO.UsersSort">
		SELECT
			users_sex,
			count(*) as `sum`
		FROM
			enroll
		GROUP BY
			users_sex
    </select>
</mapper>